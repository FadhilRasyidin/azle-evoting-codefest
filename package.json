{
    "scripts": {
        "dfx_install": "DFX_VERSION=0.15.1 sh -ci \"$(curl -fsSL https://sdk.dfinity.org/install.sh)\"",
        "replica_start": "dfx start --background",
        "replica_stop": "dfx stop",
        "canister_deploy_local": "dfx deploy azle-evoting-codefest",
        "canister_deploy_mainnet": "dfx deploy --network ic azle-evoting-codefest",
        "canister_uninstall": "dfx canister uninstall-code azle-evoting-codefest",
        "canister_call_get_message": "dfx canister call azle-evoting-codefest getMessage",
        "canister_call_set_message": "dfx canister call azle-evoting-codefest setMessage '(\"Hello world!\")'"
    },
    "dependencies": {
        "azle": "0.19.0"
    }
}

